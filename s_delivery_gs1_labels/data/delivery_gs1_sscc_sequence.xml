<?xml version="1.0" encoding="utf-8"?>

<odoo noupdate="1">
    <data>
        <record id="s_sequence_gs1_sscc_number" model="ir.sequence">
           <field name="name">SSCC Number</field>
           <field name="code">s.gs1.sscc.number</field>
           <field name="active">TRUE</field>
           <field name="padding">7</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="implementation">no_gap</field>
        </record>

         <record id="s_sequence_gs1_product_barcode" model="ir.sequence">
            <field name="name">GS1 Product barcode</field>
            <field name="code">s.gs1.product.barcode</field>
            <field name="active">TRUE</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">no_gap</field>
        </record>
    </data>
</odoo>
